{
  "name": "remark-custom-tasks",
  "version": "0.2.0",
  "description": "remark plugin for custom task markers in markdown lists",
  "type": "module",
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "index.js",
    "index.d.ts",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js"
    }
