    "demo:parsing": "node scripts/parsing-demo.js",
    "demo:serialization": "node scripts/serialization-demo.js",
    "demo:roundtrip": "node scripts/roundtrip-demo.js",
    "prepublishOnly": "npm test"
  },
  "keywords": [
    "unified",
    "remark",
    "remark-plugin",
    "markdown",
    "mdast",
    "task",
    "cannonball",
    "obsidian"
